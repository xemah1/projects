#include <stdio.h>
#include <stdlib.h>

int main(){
	int* ptr;
	int num;
	int day;
	int i,j;
	int temp;
	scanf("%d", &num);              	                  // Reading input from STDIN
	printf("Input number (array length) is %d.\n", num);					// Writing output to STDOUT
	ptr = (int*)malloc(num * sizeof(int));
	for(i=0;i<num;i++){
		scanf("%d", &day);
		// printf("Input day is %d.\n", day);

		ptr[i]=day;
	}
  
  // sorting the array
	for(i=0;i<num-1;i++){
		for(j=i+1;j<num;j++){
			if(ptr[i]<ptr[j]){
					temp=ptr[i]; ptr[i]=ptr[j]; ptr[j]=temp;
			}
		}
	}
  
 printf("%d", ptr[0]+1);
}
